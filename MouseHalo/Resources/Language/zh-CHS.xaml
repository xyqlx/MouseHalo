<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="config">设置（自动保存）</system:String>
    <system:String x:Key="unregister_task">取消任务计划注册</system:String>
    <system:String x:Key="register_task">注册开机运行</system:String>
    <system:String x:Key="active_check_interval">输入法状态自动检测间隔（ms）</system:String>
    <system:String x:Key="check_window_change">改变活动窗口时提示输入法状态</system:String>
    <system:String x:Key="check_shift_key">按下Shift时提示输入法状态（实验功能）</system:String>
</ResourceDictionary>